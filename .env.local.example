# Enclave LLC Environment Variables
# Copy this file to .env.local and fill in your values

# Site Configuration
SITE_URL=https://enclavellc.net
NEXT_PUBLIC_SITE_URL=https://enclavellc.net

# n8n Webhook Integration
# Get these from your n8n instance at n8n.ienclave.io
N8N_WEBHOOK_URL=https://n8n.ienclave.io/webhook/enclavellc-contact
N8N_WEBHOOK_TOKEN=your_secure_token_here

# Contact Information (public - shown on site)
NEXT_PUBLIC_PHONE=(424) 202-7591
NEXT_PUBLIC_EMAIL=info@enclavellc.net

# Analytics (optional)
NEXT_PUBLIC_PLAUSIBLE_DOMAIN=enclavellc.net
